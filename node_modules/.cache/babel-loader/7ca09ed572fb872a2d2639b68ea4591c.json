{"ast":null,"code":"import { computed, defineComponent } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRoute } from 'vue-router';\nimport { useScroll } from \"@vueuse/core\";\nexport default defineComponent({\n  name: \"NavBar\",\n\n  setup() {\n    const store = useStore();\n    const route = useRoute();\n    const {\n      y\n    } = useScroll(window);\n    const showNavbar = computed(() => {\n      return y.value > 0;\n    });\n    const showDownloadLink = computed(() => {\n      return route.name !== 'home' || store.state.isDownloadLinkVisible;\n    });\n    return {\n      showNavbar,\n      showDownloadLink\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAsBA,SAAQA,QAAR,EAAkBC,eAAlB,QAAwC,KAAxC;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,SAAQC,SAAR,QAAwB,cAAxB;AAEA,eAAeH,eAAe,CAAC;EAC7BI,IAAI,EAAE,QADuB;;EAE7BC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,EAAtB;IACA,MAAMM,KAAI,GAAIL,QAAQ,EAAtB;IACA,MAAM;MAAEM;IAAF,IAAQL,SAAS,CAACM,MAAD,CAAvB;IACA,MAAMC,UAAS,GAAIX,QAAQ,CAAC,MAAM;MAChC,OAAOS,CAAC,CAACG,KAAF,GAAU,CAAjB;IACD,CAF0B,CAA3B;IAGA,MAAMC,gBAAe,GAAIb,QAAQ,CAAC,MAAM;MACtC,OAAOQ,KAAK,CAACH,IAAN,KAAe,MAAf,IAAyBE,KAAK,CAACO,KAAN,CAAYC,qBAA5C;IACD,CAFgC,CAAjC;IAGA,OAAO;MACLJ,UADK;MAELE;IAFK,CAAP;EAIF;;AAhB6B,CAAD,CAA9B","names":["computed","defineComponent","useStore","useRoute","useScroll","name","setup","store","route","y","window","showNavbar","value","showDownloadLink","state","isDownloadLinkVisible"],"sourceRoot":"","sources":["/Users/jeremiahhowell/Documents/DeepDive/Website/app-website/src/components/NavBar.vue"],"sourcesContent":["<template>\n  <div class=\"container-fluid navigation\" id=\"navigation\" :class=\"{ 'active': showNavbar }\">\n    <div class=\"container\">\n      <div class=\"col left\">\n        <router-link to=\"/\" class=\"logo\">\n          <img class=\"icon\" :src=\"require('@/img/deep-dive-full-logo.png')\" alt=\"Deep Dive\">\n        </router-link>\n      </div>\n      <div class=\"col right\">\n        <!-- <div class=\"link\" data-bind=\"click: setPage.bind(this, 'updates'), css: {'': !isUpdatesPageActive(), 'active': isUpdatesPageActive()}\">Updates</div> -->\n        <router-link to=\"/help\" class=\"link\" :class=\"{ 'active': $route.name === 'help' }\">Help</router-link>\n        <div id=\"downloadNav\" :class=\"{'animation-pushLeft': showDownloadLink, 'animation-pushRight': !showDownloadLink}\">\n          <!-- <a class=\"beta button disabled\" href=\"https://forms.gle/irTSckJnmgQqPGfr7\">Beta Full</a> -->\n          <a class=\"plain\" href=\"https://apps.apple.com/us/app/deep-dive-your-fishing-guide/id1549715715\"><img class=\"download app-store\" :src=\"require('@/img/download-on-app-store.svg')\" alt=\"Download on the App Store\"></a>\n          <a class=\"plain\" href=\"https://play.google.com/store/apps/details?id=com.fishingai\"><img class=\"download play-store\" :src=\"require('@/img/google-play-badge.png')\" alt=\"Our app is now available on Google Play\"></a>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {computed, defineComponent} from \"vue\";\nimport { useStore } from \"vuex\"\nimport { useRoute } from 'vue-router'\nimport {useScroll} from \"@vueuse/core\"\n\nexport default defineComponent({\n  name: \"NavBar\",\n  setup() {\n    const store = useStore()\n    const route = useRoute()\n    const { y } = useScroll(window)\n    const showNavbar = computed(() => {\n      return y.value > 0\n    })\n    const showDownloadLink = computed(() => {\n      return route.name !== 'home' || store.state.isDownloadLinkVisible\n    })\n    return {\n      showNavbar,\n      showDownloadLink\n    }\n  }\n})\n</script>\n\n<style scoped>\n\n</style>\n"]},"metadata":{},"sourceType":"module"}